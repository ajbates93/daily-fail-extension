{
  "manifest_version": 3,
  "name": "Daily Mail Redirector",
  "version": "1.0.0",
  "description": "Redirects Daily Mail links to alternative sources",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://www.dailymail.co.uk/*",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.ts"],
    "persistent": true
  }
}
